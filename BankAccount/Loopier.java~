public class Loopier {
    public static int[] populate (int[] array) {
	for (int x = array.length - 1; x > -1; x--) {
	    array [x] = Math.random;
	}
	return array;
    }

    public static void main (String [] args) {
	int[] test = new int[5];
	System.out.println (populate (test));
    }
}
